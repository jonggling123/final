<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ddit.finalproject.team2.common.dao.Ljs_IAttendDao">
	<select id="selectAttendNo" parameterType="ddit.finalproject.team2.vo.AttendVo" resultType="string">
		SELECT ATTEND_NO FROM ATTEND
		WHERE USER_ID = #{user_id} AND LECTURE_CODE = #{lecture_code}
	</select>
</mapper>