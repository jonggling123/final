<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper	namespace="ddit.finalproject.team2.student.dao.Ljs_ILectureDao">
	<select id="selectLecture" parameterType="string" resultType="ddit.finalproject.team2.vo.LectureVo">
		SELECT LECTURE_CODE, LECTURE_NAME, USER_ID
		FROM LECTURE
		WHERE LECTURE_CODE = #{lecture_code}
	</select>
</mapper>